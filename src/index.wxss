/* pages/waveimage/waveimage.wxss */
.thumb_container {
  width: 50rpx;
  height: 50rpx;
  position: relative;
}

.v-canvas {
  position: absolute;
  bottom: 50rpx;
  right: 0;
}

.v-img {
  display: block;
  width: 100%;
  height: 100%;
  transition: transform 0.5s ease-in-out;
}

.bt-img {
  width: 100%;
  height: 100%;
}

.like-hover {
  transform: scale(1.3, 1.3);
}
